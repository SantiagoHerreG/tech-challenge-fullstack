        "remult": "^0.18.0",
        "swagger-ui-express": "^4.6.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/swagger-ui-express": "^4.1.3",
        "@vitejs/plugin-react": "^3.1.0",
        "prettier": "2.8.8",
        "tsx": "^3.12.3",
        "typescript": "^4.9.3",
        "vite": "^4.1.0"
